		"@vitest/coverage-v8": "^0.34.6",
		"@vitest/ui": "^0.34.6",
		"jsdom": "^22.1.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
